<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="power" />

    <SeekBar
        android:id="@+id/nSeekBar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:max="100"
        android:progress="50" />

    <TextView
        android:id="@+id/nTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="rx" />

    <SeekBar
        android:id="@+id/rxSeekBar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:max="100"
        android:progress="50" />

    <TextView
        android:id="@+id/rxTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ry" />

    <SeekBar
        android:id="@+id/rySeekBar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:max="100"
        android:progress="50" />

    <TextView
        android:id="@+id/ryTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="corner" />

    <SeekBar
        android:id="@+id/cornerSeekBar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:max="100"
        android:progress="50" />

    <TextView
        android:id="@+id/cornerTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.kyleduo.app.klab.m.smoothrect.SmoothRectView
        android:id="@+id/smoothRect"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>